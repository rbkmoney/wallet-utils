{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "module": "es2015",
    "target": "es5",
    "jsx": "react",
    "lib": ["es2015", "dom"],
    "baseUrl": ".",
    "paths": {
      "app/*": ["./src/app/*"]
    },
    "pretty": true
  },
  "include": [
    "./src/**/*"
  ]
}
